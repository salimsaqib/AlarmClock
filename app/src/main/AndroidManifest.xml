<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anbuilder.saqib.alarmclock;" >

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/alarmicon1"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.anbuilder.saqib.alarmclock.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="com.anbuilder.saqib.alarmclock.AlarmService"
            android:enabled="true" />

        <receiver android:name="com.anbuilder.saqib.alarmclock.AlarmReceiver" />

        <activity android:name="com.anbuilder.saqib.alarmclock.Snooze" >
        </activity>
        <activity android:name="com.anbuilder.saqib.alarmclock.Quiz" >
        </activity>
        <activity android:name="com.anbuilder.saqib.alarmclock.solution" >
        </activity>
    </application>

</manifest>
